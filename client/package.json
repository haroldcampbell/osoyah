{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint 2>&1 | tee logs/lint.log",
    "format": "prettier --write .",
    "format:check": "prettier --check . 2>&1 | tee logs/prettier.log",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false 2>&1 | tee logs/test.log",
    "e2e": "playwright test 2>&1 | tee logs/e2e.log",
    "e2e:report": "playwright show-report logs/playwright-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.17",
    "@angular/cdk": "^19.2.17",
    "@angular/common": "^19.2.17",
    "@angular/compiler": "^19.2.17",
    "@angular/core": "^19.2.17",
    "@angular/forms": "^19.2.17",
    "@angular/material": "^19.2.17",
    "@angular/platform-browser": "^19.2.17",
    "@angular/platform-browser-dynamic": "^19.2.17",
    "@angular/router": "^19.2.17",
    "@playwright/mcp": "^0.0.53",
    "dompurify": "^3.3.1",
    "marked": "^17.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.17",
    "@angular/cli": "^19.2.17",
    "@angular/compiler-cli": "^19.2.17",
    "@playwright/test": "^1.54.2",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.7.4",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.46.4"
  }
}
